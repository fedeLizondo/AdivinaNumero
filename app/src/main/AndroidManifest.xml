<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.federicolizondo.adivinanumero" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScoreActivity"
            android:label="@string/title_activity_score" />
        <activity
            android:name=".gameActivity"
            android:label="@string/title_activity_game" />
        <activity
            android:name=".InstruccionesActivity"
            android:label="@string/title_activity_instrucciones" />
        <activity
            android:name=".ingresarNombreActivity"
            android:label="@string/title_activity_ingresar_nombre"></activity>
    </application>

</manifest>
